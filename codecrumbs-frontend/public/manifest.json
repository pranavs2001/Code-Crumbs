{
  "name": "Extension Tester",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Extension Test",
  "icons": {
      "512": "logo512.png"
  },
  "browser_action": {
      "default_icon": "logo512.png",
      "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}